#include <iostream>
using namespace std;
int main()
{
    setlocale(LC_ALL, "rus");
    int i, j, N, M, a[100][100];
    cout << "Введите колличество строк N= ";
    cin >> N;
    cout << "Введите колличество столбцов M= ";
    cin >> M;
    cout << "Input matrix A \n";
    for (i = 0; i < N; i++)
        for (j = 0; j < M; j++)
            a[i][j]= rand() % 100;
    //Вывод
    cout << "matrix A \n";
    for (i = 0; i < N; i++)
    {
        for (j = 0; j < M; j++)
            cout << a[i][j] << "\t";
        cout << endl;
    }
    system("pause");
}
